{
  "manifest_version": 3,
  "name": "Test Data Seed",
  "version": "1.0.0",
  "description": "Browser extension to generate test data to be used for automated testing",
  "background": {
    "service_worker": "serviceWorker/loader.js",
    "type":"module"
  },
  "permissions": [
    "webRequest"
  ],
  "host_permissions":[
    "*://*.okx.com/"
  ],
  "icons": {
    "16": "./static/nekomata-16.png",
    "32": "./static/nekomata-32.png",
    "48": "./static/nekomata-64.png",
    "128": "./static/nekomata-128.png"
  },
  "action": {
    "default_popup": "index.html"
  }
}